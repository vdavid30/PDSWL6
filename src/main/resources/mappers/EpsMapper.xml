<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="edu.eci.pdsw.persistence.impl.mappers.EpsMapper">
    
    <select parameterType="map" id="loadALLEPS" resultMap="eps_consul">
        select
        nombre,
        nit
        FROM CM_EPS
    </select>
    <resultMap type="Eps" id="eps_consul" >            
        <result property="nombre" column="eps_nombre"/>
        <id property="nit" column="nit"/>
    </resultMap>
        
  
</mapper>